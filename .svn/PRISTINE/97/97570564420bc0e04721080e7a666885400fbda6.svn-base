.jsh-back-top a { position: relative; display: block; background: #fff; color: #999; line-height: 18px; height: 36px; font-size: 14px; text-align: center; padding: 7px 5px; }
.jsh-back-top a span { display: none; *display: block; }
.jsh-back-top a:before { content: ""; display: block; width: 0; height: 0; position: absolute; left: 7px; top: 15px; border-width: 0 18px 18px; border-style: dashed dashed solid; border-color: transparent transparent #999; }
.jsh-back-top a:after { content: ""; display: block; width: 0; height: 0; position: absolute; left: 7px; top: 17px; border-width: 0 18px 18px; border-style: dashed dashed solid; border-color: transparent transparent #fff; }
.jsh-back-top:hover a span { display: block; }
.jsh-back-top:hover a:before,
.jsh-back-top:hover a:after { display: none; }

