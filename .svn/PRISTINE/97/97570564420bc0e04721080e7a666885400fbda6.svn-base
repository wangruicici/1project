
body { color:#888888; font: 14px/1.6 "Microsoft Yahei",simsun,"sans serif",tahoma,verdana,helvetica; }
a { color:#555555; }
a:hover { color: #bf0a10; text-decoration:none; }

/* Layout */
#tn-doc { padding-top: 78px; overflow: hidden; }
.sow-inner-page #tn-doc { padding-top: 0; }
.sow-container { width: 1140px; padding-right: 15px; padding-left: 15px; margin-right: auto; margin-left: auto; }

@media (min-width: 768px) {
    .sow-container { width: 720px; }
}
@media (min-width: 992px) {
    .sow-container { width: 940px; }
}
@media (min-width: 1200px) {
    .sow-container { width: 1140px; }
}

@media (max-width: 640px) {
    #tn-doc { padding-top: 47px; }
    .sow-container { width: auto; }
    .owl-theme .owl-controls .owl-page span { width: 4px; height: 4px; }
    #fp-nav.right { right: 0!important; }
    /*#qiao-wrap { display: none!important; }*/
    ins#qiao-invite-wrap { display: none !important; visibility: hidden !important; }
}

/* Header */
#tn-header { position: fixed; width: 100%; top: 0; z-index: 100001; }
.tn-header-bg .sow-e-talk { position: absolute; right: 0; padding: 20px 25px 19px; line-height: 40px; font-size: 16px; transition: all .5s ease-in 0s; *width: 170px; }
.tn-header-bg .sow-e-talk:hover { background: #bf0a10; color: #fff; }
.tn-header-bg .sow-e-talk i { float: right; margin-left: 10px; width: 40px; height: 40px; background: url("e-talk.png")/*tpa=http://www.jinhusns.com/Themes/Channel/Default/Appearances/Default/Images/e-talk.png*/ no-repeat center center; background-size: cover; transition: all .5s ease-in 0s; }
.tn-header-bg .sow-e-talk em {  }
.tn-header-bg { border-bottom: 1px #eee solid; background: #fff; transition: all .5s ease-in 0s; }
.tn-header-bg .sow-logo{ float:left; padding:19px 0; transition: all .5s ease-in 0s; }
.tn-header-bg .sow-logo a{ display: block; width: 100px; height: 40px; background-image: url("spb_logo.png")/*tpa=http://www.jinhusns.com/Themes/Channel/Default/Appearances/Default/Images/spb_logo.png*/; background-position: center center; background-repeat: no-repeat; background-size: cover; text-indent: -9999px; transition: all .5s ease-in 0s; }
.sow-inner-page #tn-header { position: absolute; }

@media (max-width: 640px) {
    .tn-header-bg .sow-logo { padding: 8px 0; float: none; display: inline-block; }
    .tn-header-bg .sow-logo a { width: 73px; height: 30px; }
}
@media (max-width: 1400px) {
    .tn-header-bg .sow-e-talk  { display: none; }
}
@media (max-width: 1600px) {
    .tn-header-bg .sow-e-talk em { display: none; }
}

/* Navigation */
.sow-nav { font-size:15px; text-align: right; *overflow: hidden; zoom:1; }
.sow-nav .sow-menu { *padding-top: 26px; }
.sow-nav .sow-menu li{ display: inline-block; *display: inline; float: none; }
.sow-nav .sow-menu li a { padding: 26px 15px; display: inline-block; *display: inline; font-size: 16px; float: none; border-bottom: transparent solid 1px; _border-bottom: #FFF solid 2px; transition: padding .5s ease-in 0s; }
.sow-nav .sow-menu li.tn-selected a,
.sow-nav .sow-menu li a:hover{ color:#bf0a10; border-bottom:#bf0a10 solid 2px; margin-bottom:-1px; _margin-bottom:0px; }
.sow-nav .sow-menu li a.tn-drop-link { padding-right: 25px; }
.sow-nav .sow-menu li a.tn-drop-link:hover { border-bottom: none; }

.sow-nav .tn-drop-menu li .tn-sub-container { box-shadow: 0 0 0; }
.sow-nav .tn-drop-menu li .tn-sub-container .tn-sub { border-radius: 0; border: #ddd solid 1px; border-width: 0 1px 1px; }
.sow-nav .tn-drop-menu li .tn-sub-container .tn-sub a { display: block; text-align: left; border-width: 0; white-space: nowrap; margin-bottom: 0; padding: 10px 13px;  }

.tn-header-fixed { background: rgba(255, 255, 255, .95); }
.tn-header-fixed .sow-e-talk { padding: 8px 25px; line-height: 30px; }
.tn-header-fixed .sow-e-talk i { width: 30px; height: 30px; }
.tn-header-fixed .sow-logo { padding: 8px 0; *padding: 3px 0; padding: 3px 0\9; }
.tn-header-fixed .sow-logo a { width: 75px; *width: 100px; width: 100px\9; height: 30px; *height: 40px; height: 40px\9; }
.tn-header-fixed .sow-nav .sow-menu { *margin-top: -15px; }
.tn-header-fixed .sow-nav .sow-menu li > a { padding-top: 10px; padding-bottom: 10px; }

.sow-nav .sow-menu-swich { display: none; }

@media (max-width: 640px) {
    .sow-top { text-align: center; }
    .sow-nav { text-align: left; position: absolute; top: 38px; left: 0; transition: all .5s ease-in 0s; }
    .sow-nav .sow-menu-swich { display: block; position: absolute; top: -30px; left: 15px; width: 30px; height: 30px; transition: all .5s ease-in 0s; }
    .sow-nav .sow-menu-swich i { display: block; height: 2px; background: #888; margin: 6px 5px; transition: all .5s ease-in 0s; }
    .sow-nav .sow-menu { width: 120px; position: absolute; z-index: 100001; top: 8px; left: -120px; padding: 10px 0; background: rgba(0, 0, 0, .95); transition: all 0.5s ease; }
    .sow-nav .sow-menu.active { left: 0; }
    .sow-nav .sow-menu li { display: block; }
    .sow-nav .sow-menu li a { display: block; padding: 8px 10px; color: #fff; font-size: 15px; }
    .sow-nav .sow-menu li.tn-selected a,
    .sow-nav .sow-menu li a:hover { border-bottom-width: 0; border-right: #bf0a10 solid 2px; }

    .tn-header-fixed .sow-logo { padding: 5px 0; }
    .tn-header-fixed .sow-logo a { width: 63px; height: 25px; }
    .tn-header-fixed .sow-nav { top: 32px; }
    .tn-header-fixed .sow-nav .sow-menu-swich { top: -25px; width: 25px; height: 25px; }
    .tn-header-fixed .sow-nav .sow-menu-swich i { margin: 4px 5px; }
}

/* Top Sub Menu */
.sow-overview-menu { position: absolute; z-index: 100000; width: 100%; left: 0; top: 81px; text-align: center; padding: 20px 0;  transition: all 0.5s ease-in 0s; }
.sow-fix-top { background: rgba(255, 255, 255, .95); position: fixed; top: 0; box-shadow: 0 0 3px #bbb; }
.sow-overview-menu li { display: inline; }
.sow-overview-menu li a { padding: 0 20px; }
.sow-overview-menu li.active a { color: #bf0a10; }

@media (max-width: 640px) {
    .sow-overview-menu { top: 48px; }
    .sow-overview-menu li a { font-size: 13px; padding: 0 5px; white-space: nowrap; }
    .sow-fix-top { top: 0; padding: 10px 0; }
}

/* Home Page
-----------------------------------------*/

/* slider */
.sow-slider { position: relative; clear: both; text-align: center; }
.sow-slider .item > a { display: block; overflow: hidden; }
.sow-slider img { width: 100%; max-width: 1140px; }
.sow-slider .sow-slider-train { background: #151b3b; }
.sow-slider .sow-slider-product { background: #128d94; }
.sow-slider .sow-slider-fans { background: url("slider03_bg.jpg")/*tpa=http://www.jinhusns.com/Themes/Channel/Default/Appearances/Default/Images/slider03_bg.jpg*/ no-repeat; background-size:cover;  }
.sow-slider .sow-slider-culture { background: #1fa4c7 url("slider04_tr.png")/*tpa=http://www.jinhusns.com/Themes/Channel/Default/Appearances/Default/Images/slider04_tr.png*/ no-repeat right top;  }
.sow-slider .sow-slider-culture a { background: url("slider04_lb.png")/*tpa=http://www.jinhusns.com/Themes/Channel/Default/Appearances/Default/Images/slider04_lb.png*/ no-repeat left bottom; }

/* Download */
.sow-outline { padding:10px 0; position: absolute; width: 100%; bottom: 50px; }
.sow-outline .sow-text { width: 66.6666666%; float: left; color: #fff; text-align: left; }
.sow-outline p { font-size: 24px; text-shadow: 1px 1px 1px rgba(0, 0, 0, .5); }
.sow-outline h1 { font-size:48px; font-weight:normal; margin: 0 0 10px; line-height: 1; text-shadow: 1px 1px 1px rgba(0, 0, 0, .5); }
.sow-outline .sow-download { width: 33.3333333%; float: right; padding-top: 20px; }
.sow-outline .sow-download .sow-btn-area { width: 50%; float: left; }
.sow-outline a { display: block; padding: 15px 0; margin: 0 15px; font-size: 18px; line-height: 1; text-align: center; border: 2px #fff solid; color: #fff; background: rgba(0, 0, 0, 0.2); transition: all .5s ease-in 0s; }
.sow-outline a:hover { color: #bf0a10; background: rgba(255, 255, 255, .8); }
@media (max-width: 640px) {
    .sow-outline { position: static; padding: 20px 0 10px; }
    .sow-outline .sow-text { color: #555; width: 50%; }
    .sow-outline p { font-size: 14px; text-shadow: 0 0 0; }
    .sow-outline h1 { font-size: 18px;text-shadow: 0 0 0; }
    .sow-outline .sow-download { width: 50%; padding-top: 5px; }
    .sow-outline a { padding: 10px 0; margin: 0 5px; font-size: 13px; border-color: #888; background: transparent; border-width: 1px; color: #555; }
}
@media (max-width: 320px) {
    .sow-outline p { font-size: 11px; }
    .sow-outline a { font-size: 12px; }
}

