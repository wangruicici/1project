.sow-slider-img { background: url("sow-function-bg.png")/*tpa=http://www.jinhusns.com/Themes/Channel/Default/Appearances/Default/Images/sow-function-bg.png*/ repeat; background-color: rgba(0,0,0,0.1); width: 63%; border: 1px solid #eaeaea; padding: 25px; margin: auto; }
.sow-respose-container { text-align: center; margin: 0 auto; width: 75%; padding: 0 15px; height: 100%; position: relative; }
.sow-respose-container h2 { font-size: 60px; font-weight: normal; margin: 120px 0 30px; }
.sow-respose-container p { font-size: 18px; margin: 30px 0; color: #888; -webkit-animation-name: bounceInDown; animation-name: bounceInDown; }
.sow-function-content {top:120px;position:relative;}

@-webkit-keyframes bounceInDown {
    from, 60%, 75%, 90%, to { -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000); animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000); }
    0% { opacity: 0; -webkit-transform: translate3d(0, -3000px, 0); transform: translate3d(0, -3000px, 0); }
    60% { opacity: 1; -webkit-transform: translate3d(0, 25px, 0); transform: translate3d(0, 25px, 0); }
    75% { -webkit-transform: translate3d(0, -10px, 0); transform: translate3d(0, -10px, 0); }
    90% { -webkit-transform: translate3d(0, 5px, 0); transform: translate3d(0, 5px, 0); }
    to { -webkit-transform: none; transform: none; }
}
@keyframes bounceInDown {
    from, 60%, 75%, 90%, to { -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000); animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000); }
    0% { opacity: 0; -webkit-transform: translate3d(0, -3000px, 0); transform: translate3d(0, -3000px, 0); }
    60% { opacity: 1; -webkit-transform: translate3d(0, 25px, 0); transform: translate3d(0, 25px, 0); }
    75% { -webkit-transform: translate3d(0, -10px, 0); transform: translate3d(0, -10px, 0); }
    90% { -webkit-transform: translate3d(0, 5px, 0); transform: translate3d(0, 5px, 0); }
    to { -webkit-transform: none; transform: none; }
}
.bounceInDown { -webkit-animation-name: bounceInDown; animation-name: bounceInDown; }

