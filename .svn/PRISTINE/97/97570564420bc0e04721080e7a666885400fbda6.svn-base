/* Heading */
.sow-heading { text-align: center; margin: 0 0 30px; }
.sow-heading h3 { font-size: 28px; font-weight: normal; display: inline-block; position: relative; }
.sow-heading h3::before,
.sow-heading h3::after { content: ""; position: absolute; width: 85%; top: 50%;  height: 4px; border-style: dotted; border-width: 1px 0; border-color: #555; }
.sow-heading h3::before { right: 100%; margin: -3px 15px 0 0; }
.sow-heading h3::after { left: 100%; margin: -3px 0 0 15px; }

@media (max-width: 640px) {
    .sow-heading { margin: 0; }
    .sow-heading h3 { font-size: 18px; margin: 10px 0 20px; }
    .sow-heading h3::before,
    .sow-heading h3::after { width: 50%;  }
}

/* guide */
.sow-guide-box { padding: 30px 0 80px; background: #fafafa; border: 1px rgba(0, 0, 0, .1) solid; border-width: 1px 0; text-align: center; }
.sow-guide-box ul { height: 320px;  }
.sow-guide-box li { width: 25%; float: left; }
.sow-guide-box .sow-wrap { position: relative; height: 320px; }
.sow-guide-box .sow-inner { position: absolute; top: 0; left: 15px; right: 15px; height: 260px; padding: 30px 15px; backface-visibility: hidden; transform-style: preserve-3d; transition: all 0.4s ease-in-out 0s; text-align: center; }
.sow-guide-box .sow-front { background: #fff; z-index: 2; transform: rotateX(0deg) rotateY(0deg); color: #888; }
.sow-guide-box li:hover .sow-front { transform: rotateY(-180deg); z-index: 1;}
.sow-guide-box strong { display: block; font-size: 18px; font-weight: normal; margin-bottom: 20px; }
.sow-guide-box p { margin-bottom: 20px; font-size: 16px; }
.sow-guide-box .sow-front p { display: none; }
.sow-guide-box .sow-back { z-index: 1; transform: rotateY(-180deg); color: #fff; }
.sow-guide-box .sow-back p { height: 9em; }
.sow-guide-box .sow-back a { display: block; padding: 10px; color: #fff; }
.sow-guide-box li:hover .sow-back { transform: rotateY(0deg); z-index: 2; }
.sow-guide-box i { display: inline-block; height: 120px; width: 120px; margin: 40px auto 20px; border-radius: 50%; background: url("app-icon.png")/*tpa=http://www.jinhusns.com/Themes/Channel/Default/Appearances/Default/Images/app-icon.png*/; }
.sow-guide-box .sow-guide-first i { background-position: -480px -240px; background-color: #00bcd4; }
.sow-guide-box .sow-guide-second i { background-position: -600px -240px; background-color: #8bc34a; }
.sow-guide-box .sow-guide-third i { background-position: -720px -240px; background-color: #ff9800; }
.sow-guide-box .sow-guide-fourth i { background-position: -840px -240px; background-color: #ec407a; }
.sow-guide-box .sow-guide-first .sow-back { background: #00bcd4; }
.sow-guide-box .sow-guide-first .sow-back a { background: #00acbf; border: #009eaf 1px solid; }
