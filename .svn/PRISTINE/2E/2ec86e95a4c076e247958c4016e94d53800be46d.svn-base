html,body,div,span,iframe,map,font,img,a img,samp,strong,hr,
h1,h2,h3,h4,h5,h6,b,p,blockquote,a,address,code,dl,dt,dd,ol,
ul,li,form,label,table,tr,td,th{padding:0;margin:0;border:0;list-style: none;font-family: Microsoft YaHei}
body {
  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif,Microsoft YaHei;
  padding-bottom: 50px;
}
.bs-docs-masthead,
.bs-docs-header {
  color: #cdbfe3;
  text-shadow: 0 1px 0 rgba(0, 0, 0, 0.1);
  background-color: #6f5499;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#563d7c), to(#6f5499));
  background-image: -webkit-linear-gradient(top, #563d7c 0, #6f5499 100%);
  background-image: -o-linear-gradient(top, #563d7c 0, #6f5499 100%);
  background-image: linear-gradient(to bottom, #563d7c 0, #6f5499 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#563d7c', endColorstr='#6F5499', GradientType=0);
  background-repeat: repeat-x;
}
.bs-docs-masthead h1 {
  font-weight: 500;
}
.page-header {
  text-align: left;
}
#github {
  display: none;
  position: fixed;
  width: 150px;
  height: 150px;
  top: 0;
  right: 0;
  z-index: 2000;
}
.bottom {
  border-top: 1px solid #eee;
  margin-top: 40px;
  padding-top: 20px;
  padding-bottom: 20px;
  text-align: center;
}
.hljs {
  background-color: transparent;
}
@media (min-width: 768px) {
  #github {
    display: block;
  }
}
/********************************************************/
.page-scroll{font-family:Microsoft YaHei;}
.coach-title{width: 100%;padding: 20px;text-align: center;height: auto;}
.coach-title-top{font-size: 50px;line-height: 50px;text-align: center;color: #09c7ff;padding: 10px;margin-top: 30px }
.coach-title-bottom{font-size: 20px;line-height: 50px;text-align: center;color: #09c7ff;padding: 10px;}
.position-r{position:relative}
.position-a{position:absolute;left:0;bottom:0}

.coach{height: 400px;}
.coach-top{height:30%;background: green}
.coach-bottom{height: 70%;background:palevioletred}

.coach-top-left{width: 40%;height:100%;background: black;float: left;overflow: hidden}
.coach-top-right{width: 60%;height:100%;background: red;float: left;overflow: hidden}
.coach-bottom-left{width: 70%;height:100%;background: blue;float: left;overflow: hidden}
.coach-bottom-right{width: 30%;height:100%;background: green;float: left;overflow: hidden}
/*detail*/
.detail{width: 100%;height: 0;background: #09c7ff
}
.man-1{font-size: 20px;line-height: 20px;font-weight: 400;color: white;padding: 5px 30px}
.man-2{font-size: 14px;line-height: 20px;font-weight: 400;color: white;padding: 2px 30px}
/*活动模块*/
.active{height: 400px;overflow: hidden;margin-top: 30px}
.active-detail{width: 100%;height: 100%;overflow: hidden;position: relative;}
.active-d{width: 100%;height: 100%;background: rgba(255, 255, 255, 0.8);position: absolute;left: -100%;top: 0; }
.active-title-top{font-size: 24px;line-height: 24px;color: #01A89E;padding: 10px;margin-top: 70px }
.active-title-bottom{font-size: 16px;line-height: 24px;color: #01A89E;padding: 10px;margin-top: 180px}
/*找工作*/
.offer{height: 450px;background: #f4f4f4;overflow: hidden;}
.offer-top{font-size: 40px;font-weight: bold;color: #ff4d56;line-height: 40px;margin: 30px 0;border-bottom: 1px solid #000000;padding-bottom: 20px}
.offer-middle>ul>li{font-size: 24px;color: #9f9f9f;padding: 10px;cursor: pointer}
.offer-middle>ul>li:hover{background: #FF4D56;color: #ffffff}
.offer-middle>ul>li>span{font-size: 24px;color: #09c7ff;padding: 10px;}
.offer-box{width: 100%;height: 100%;position: absolute;left:0;top:500px;}
.offer-title{padding: 30px;font-size: 40px;line-height: 40px;font-weight: bold;padding-top: 110px;}
.offer-detail{padding:30px;font-size: 20px;line-height: 40px;position: absolute;
  left: 0;  top: 160px;}
.footer-box{width: 100%;height: auto;padding: 20px;background: #000;font-size: 18px;color: white}
.textar{text-align: right}
.huodong{height: 500px;text-align: center}
.huodongl{margin-left: 40%;}
.huodongll{margin-left: 20%;}
.huodong>img{height: 100%;}
