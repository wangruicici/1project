}
.fenge{
  width:200px;
  margin: 15px auto;
  text-align: center;
  position: relative;
  &:before,&:after{
    content: "";
    position: absolute;
    width: 50px;
    top: 8px;
    height: 4px;
    border-style: dotted;
    border-width: 1px 0;
    border-color: #555;
  }
  &:before{
    right:0px;
  }
  &:after{
    left:0px;
  }
}
/*项目经验*/
@about-h:400px;
@section-title-h:60px;
.overflow{
  overflow:hidden;
}
/*****************************************about*/
.product-three{
  .div-vertical-align{
    padding:20px 0;
  }
}

.about{
  width:100%;
  max-width: 800px;
  margin: 0 auto;
  clear: both;
  height:@about-h - @section-title-h;
      &-inner{
        margin-top: @section-title-h/2;
        height: 100%;
        .jianrong(@type: box-sizing,@type-value: border-box);
        overflow: hidden;

        &>img{
          width: 100%;
        }
        &-li{
          width: 100%;
          height: 20px;
          margin-top: 10px;
          //border:1px solid @bg-five-s;
          color:#fff;
          font-size: 1rem;
          font-family: @font-family-yaHei;
        }
        &-li:nth-child(1){
          font-size: 3rem;
          font-family: sketchrockwell;
          color:@color-Yf6
        }
      }
}
/***********************************************skill*/
.product-four{
  .section-title{
      width:245px;
  }
  .section-title-thWord{
    width:130px;
    span:nth-child(1){
      margin-left: 20px;
    }
  }
}
.skill{
  max-width: @width-800;
  margin:  0 auto;
  margin-top: 50px;
  color:#fff;
  font-size: 2rem;
  font-family: @font-family-yaHei;
  text-align: center;
  @skill-innerW:500px;
  @skill-innerH:50px;
  &-inner{
    width: @skill-innerW;
    padding:20px 10px;
    //border:1px solid @color-Yf6;
    margin:  0 auto;
    text-align: center;
    &-box{
      font-family: @graydesign;
      padding: 5px 15px;
      font-size: 1.5rem;
      border-radius: 3px;
      background: @bg-one-s;
      color: #fff;
      display: inline-block;
      margin: 5px;
      cursor: pointer;
      -webkit-animation-duration: 1s ;
      animation-duration: 1s;
      -webkit-animation-fill-mode: both;
      animation-fill-mode: both;
    }

    &-box:hover{
      background: @bg-one-e;
      animation-name: rubberBand;
    }
  }
}
