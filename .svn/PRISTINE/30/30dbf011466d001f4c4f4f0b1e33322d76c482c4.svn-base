}
.fp-tooltip{
  opacity: 1!important;
}
.section-title{
  font-family: @graydesign;
  font-size: 8rem;
  width: 230px;
  height:60px;
  margin:0 auto;
  text-shadow: 55px 55px 55px #ffffff;
  color:@color-Yf6;
    &-fWord{
      font-size: 10rem;
      height: 60px;
      line-height: 60px;
      float: left;
    }
   &-tWord{
      font-size: 1rem;
      font-family: @font-family-yaHei;
      height: 20px;
     line-height: 20px;
    }
    &-thWord{
      font-size: 5rem;
      height:40px;
      line-height: 40px;
      display: -webkit-flex;
      display: flex;
      justify-content:space-between;
      width:120px;
    }
    &-tWord,&-thWord{
      float: left;
    }
}
.section-img-box{
  max-width: 600px;
  //overflow: hidden;
  margin: 0 auto;
  margin-top: 30px;
}
@section-img-h:180px;
.section-img{
  height: @section-img-h;
  overflow: hidden;
  padding:5px;
  &>div{
    width:100%;
    height:100%;
    font-family: @font-family-yaHei;
    text-align: center;
    line-height: @section-img-h;
    color:#fff;
    font-size: 3rem;
    text-shadow: #737373 5px 5px 5px;
    cursor: pointer;
    position: relative;
    &>div{
      width:100%;
      height:100%;
      background:url(../img/n1.jpg);
      background-color: transparent;
      background-size: cover;
      background-position: center center;
      background-repeat: no-repeat;
      font-family: @font-family-yaHei;
      -webkit-filter:blur(1px);
      position: absolute;
      left: 0;
      top: 0;
      z-index: 0;

    }
    .one1{
      background:url(../img/n1.jpg);
    }
    .one2{
      background:url(../img/n2.jpg);
    }
    .one3{
      background:url(../img/n3.jpg);
    }
    .one4{
      background:url(../img/n4.jpg);
    }
    &>span{
      transition: opacity 200ms ease-in;
      display: block;
      width:100%;
      height:100%;
      position: absolute;
      left: 0;
      top: 0;
      z-index: 1;
    }
  }
}
.modal-backdrop{
  position: relative;
}


