/*top 开始*/
.top-box{
    width:100%;
	height:36px;
	background:#fafafa;
	border-bottom:1px solid #dfdfdf;
}
.top{
	height:100%!important;
	margin:0 auto;
	color:#8c8c8c;
	font-size:12px;
	font-family:"微软雅黑";
}
.top-left{
	float:left;
	height:100%;
	line-height:36px;
}
.top-right{
	float:right;
	height:100%;
	line-height:36px;
}
.top a{
	color:#8c8c8c;
	text-decoration:none;
	transition:all 0.5s ease;
}
.top a:hover{
	color:black;
}
.top span{
	color:#dfdfdf;
	margin:0 10px;
}
/*top 结束*/

/*logo 开始*/
.logo-box{
	height:110px!important;
	margin:0 auto;
}
.logo{
	float:left;
	width:70px;
	height:70px;
	margin-top:25px;
	transition:all 1s ease;
	-webkit-transition:all 1s ease;
}
.logo:hover{
	transform:rotate(360deg);
	-webkit-transform:rotate(360deg);
}
.logo-right{
	float:right;
	width:auto;
	height:100%;
}
.logo-r-top{
	width:auto;
	height:50%;
	line-height:55px;
}
.logo-r-top a{
	float:right;
	font-size:12px;
	color:#8c8c8c;
	padding-left:20px;
	background:url(../img/clock.jpg) no-repeat 0 center;
}
.logo-r-top a:hover{
	color:#ff4a00;
	background:url(../img/red-clock.jpg) no-repeat 0 center;
}
.logo-r-bottom{
	width:auto;
	height:50%;
}
.logo-r-b-search{
	float:left;
	width:auto;
	height:100%;
}
.logo-r-b-search input{
	float:left;
	width:370px;
	height:40px;
	padding-left:15px;
	font-family:"宋体";
	font-size:16px;
	color:#8c8c8c;
	border:1px solid #e3e3e3;
}
.logo-r-b-search a{
	display:inline-block;
	float:left;
	width:40px;
	height:40px;
	border:1px solid #e3e3e3;
	border-left:0;
	background:url(../img/search.jpg) no-repeat center center;
}
.logo-r-b-search a:hover{
	background:#ff4a00 url(../img/red-search.jpg) no-repeat center center;
	border:1px solid #ff4a00;
	border-left:0;
}
.logo-r-b-shopcart{
	float:left;
	width:110px;
	height:42px;
	line-height:42px;
	text-align:center;
	font-size:14px;
