.ml-nav {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 0;
}
.section .container {
  height: 100%;
}
.navbar-brand {
  font-family: sketchrockwell ;
  font-size: 3rem;
  color: #f6b908;
}
.navbar-brand,
.navbar-brand:hover {
  color: #f6b908 !important;
}
.index-box.div {
  display: table-cell;
  vertical-align: middle;
  width: 100%;
  height: 100%;
}
body {
  min-height: 700px!important;
}
.section {
  height: 100%;
}
.div {
  display: table-cell;
  vertical-align: middle;
  height: 500px;
  width: 1000px;
}
.div-vertical-align {
  width: 90%;
  height: 70%;
  position: relative;
  margin: 0 auto;
}
.border-box-rt,
.border-box-lb {
  position: absolute;
  width: 100px;
  height: 100px;
}
.border-box-rt {
  right: 0;
  top: 0;
  border-right: 3px solid #fff;
  border-top: 3px solid #fff;
}
.border-box-lb {
  left: 0;
  bottom: 0;
  border-left: 3px solid #fff;
  border-bottom: 3px solid #fff;
}
.font-family-sketchrockwell {
  font-family: sketchrockwell;
}
.font-family-design {
  font-family: graydesign;
}
.product-one .div-vertical-align {
  font-size: 10rem;
  text-align: center;
}
.index-box > .div-vertical-align {
  line-height: 1.2;
  height: 50%;
  color: #f6b908 !important;
  width: 630px;
  text-align: left;
}
.ling-gan {
  font-size: 3rem;
  text-align: left;
  font-family: Microsoft YaHei, SimSun, Arial, Helvetica, sans-serif;
  color: #fff;
}
.fu-title {
  font-size: 3rem;
  text-align: right;
  font-family: graydesign;
  color: #fff;
}
.fp-tooltip {
  opacity: 1!important;
}
.section-title {
  font-family: graydesign;
  font-size: 8rem;
  width: 210px;
  height: 60px;
  margin: 0 auto;
  color: #f6b908;
}
.section-title-fWord {
  font-size: 10rem;
  height: 60px;
  line-height: 60px;
  float: left;
}
.section-title-tWord {
  font-size: 1rem;
  font-family: Microsoft YaHei, SimSun, Arial, Helvetica, sans-serif;
  height: 20px;
  line-height: 20px;
}
.section-title-thWord {
  font-size: 5rem;
  height: 40px;
  line-height: 40px;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  width: 120px;
}
.section-title-tWord,
.section-title-thWord {
  float: left;
}
.section-img-box {
  max-width: 600px;
  overflow: hidden;
  margin: 0 auto;
  margin-top: 30px;
}
.section-img {
  height: 180px;
  overflow: hidden;
  margin: 2px 0!important;
  padding: 3px;
}
.section-img > img {
  width: 100%;
}
.about {
  width: 100%;
  max-width: 800px;
  margin: 0 auto;
  height: 340px;
}
.about-inner {
  margin-top: 30px;
  height: 100%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  overflow: hidden;
}
.about-inner > img {
  width: 100%;
}
.about-inner-li {
  width: 100%;
  height: 20px;
  margin-top: 10px;
  color: #fff;
  font-size: 1rem;
  font-family: Microsoft YaHei, SimSun, Arial, Helvetica, sans-serif;
}
.about-inner-li:nth-child(1) {
  font-size: 3rem;
  font-family: sketchrockwell;
  color: #f6b908;
}
.product-four .section-title {
  width: 225px;
}
.product-four .section-title-thWord {
  width: 130px;
}
.product-four .section-title-thWord span:nth-child(1) {
  margin-left: 20px;
}
.skill #indicatorContainerWrap,
.skill #indicatorContainer {
  display: inline-block;
  position: relative;
}
.skill #prgLogo {
  position: absolute;
  width: 60px;
  height: 60px;
  margin-top: -30px;
  margin-left: -30px;
}
.skill-rad-prg {
  position: relative;
  height: 120px;
}
.skill-div {
  position: absolute;
  top: 45px;
  left: 0;
  width: 100%;
  height: 30px;
  text-align: center;
  font-size: 3rem;
  color: #f6b908;
}
